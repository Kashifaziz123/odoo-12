<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">

    <t t-extend="ActionpadWidget">
        <t t-jquery="div.actionpad" t-operation="before">
            <div class="button-multi-uom">
                <span class="multi-uom-span">
                    Prescription
                </span>
            </div>
        </t>
    </t>

    <t t-name="MultiUomWidget">
        <div class="modal-dialog">
            <div class="popup widget order-scroller touch-scrollable" style="width:70%;height: 545px;font-size: 13px;">
                <p class="title">Select Unit of Measure</p>
                <br/><br/>
                 <table width="100%">
                     <tr>
                         <td width="10%" style="padding:5px; text-align:left;"><label for="fname">Optometrist:</label></td>
                         <td width="90%" style="padding:5px;"> <input style="height:30px;" type="text"  name="lname"/></td>
                     </tr>
                      <tr>
                         <td width="10%" style="padding:5px; text-align:left;"><label for="fname">Client:</label></td>
                         <td width="90%" style="padding:5px;"> <input style="height:30px;" type="text"  name="lname"/></td>
                     </tr>
                      <tr>
                         <td width="10%" style="padding:5px; text-align:left;"><label for="fname">Checkup Date:</label></td>
                         <td width="90%" style="padding:5px;"> <input style="height:30px;" type="text"  name="lname"/></td>
                     </tr>
                      <tr>
                         <td width="10%" style="padding:5px; text-align:left;"><label for="fname">Test Type:</label></td>
                         <td width="90%" style="padding:5px;"> <input style="height:30px;" type="text"  name="lname"/></td>
                     </tr>
                 </table>







                <div class="footer">
                    <div class="button confirm">
                        Ok
                    </div>
                    <div class="button cancel">
                        Cancel
                    </div>
                </div>
            </div>
        </div>
    </t>

</templates>